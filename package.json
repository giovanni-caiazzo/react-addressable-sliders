{
    "name": "react-addressable-sliders",
    "version": "0.0.4",
    "description": "Multi-range slider made with react with addressable and identifiable ranges",
    "private": false,
    "author": "Giovanni Caiazzo",
    "main": "lib/index.js",
    "license": "MIT",
    "repository": {
        "type": "git",
        "url": "https://github.com/giovanni-caiazzo/react-addressable-sliders.git"
    },
    "keywords": [
        "react",
        "slider",
        "multi-range",
        "addressable"
    ],
    "devDependencies": {
        "@babel/cli": "^7.8.4",
        "@babel/plugin-proposal-class-properties": "^7.14.5",
        "@babel/preset-env": "^7.9.5",
        "@babel/preset-react": "^7.14.5",
        "@babel/core": "^7.14.6",
        "babel-preset-react": "^6.24.1",
        "husky": "^6.0.0",
        "prettier": "^2.3.1",
        "pretty-quick": "^3.1.0",
        "react": "^17.0.2",
        "react-dom": "^17.0.2"
    },
    "dependencies": {
        "prop-types": "^15.7.2"
    },
    "scripts": {
        "build": "babel ./src -d lib --config-file ./.babelrc",
        "format": "yarn run format:prettier",
        "format:prettier": "node_modules/.bin/prettier --config .prettierrc \"src/**/*.*\" --write",
        "publish-new-patch": "npm version patch && npm publish"
    }
}
